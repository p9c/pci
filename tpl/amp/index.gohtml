{{define "title"}}title: Rental Mobil Surabaya Termurah dan Terbaik- KLIK AJA - CS buka 24 jam{{end}}
{{define "canonical"}}canonical: https://tran99.com/index.html{{end}}
{{define "photos"}}/s/imgs/mainbgs.jpg{{end}}
{{define "content"}}
<main class=" ">
    <section class="slider flex items-center justify-center ">
        <div class="txwhite">
            <h1 class="center">{{ .Home.Title }}</h1>
            <h2 class="center">{{ .Home.SubTitle }}</h2>
        </div>
    </section>
    <section class="flex flex-wrap hvw">
        <div class="px2 col-12 sm-col-6 bgblack flex items-center justify-center">
         {{ template "logoround"}}
        </div>
        <div class="px2 col-12 sm-col-6 flex items-center justify-center ">
            <div class="mx4 ">
                <h3 class="text-title">{{ .Home.Welcome }}</h3>
                <p class="text-center"> {{ .Home.About }}</p>
                <button class="btn bgblue txwhite">Get App</button>
            </div>
        </div>
    </section>
    <section class="flex flex-wrap bggray py4">
        <h3 class="col-12 center"> {{ .Home.Features }}</h3>
        <div class="px2 col-12 sm-col-6 md-col-3 flex flex-column justify-center center">
            {{ template "puzzle"}}
            <h6>{{ .Home.Feature1 }}</h6>
            <p>{{ .Home.Feature1txt }}</p>
        </div>
        <div class="px2 col-12 sm-col-6 md-col-3 flex flex-column justify-center center">
            {{ template "clock"}}
            <h6 class="shape-text">{{ .Home.Feature2 }}</h6>
            <p>{{ .Home.Feature2txt }}</p>
        </div>
        <div class="px2 col-12 sm-col-6 md-col-3 flex flex-column justify-center center">
            {{ template "addressbook"}}
            <h6 class="shape-text">{{ .Home.Feature3 }}</h6>
            <p> {{ .Home.Feature3txt }}</p>
        </div>
        <div class="px2 col-12 sm-col-6 md-col-3 flex flex-column justify-center center">
            {{ template "folder"}}
            <h6 class="shape-text">{{ .Home.Feature4 }}</h6>
            <p>{{ .Home.Feature4txt }}</p>
        </div>
    </section>
    <section class="flex flex-wrap py4 center">
        <h3 class="col-12 center">{{ .Home.Gallery }}</h3>
        <div class="XXXXXXXXXX">
        XXXXXXXXXXXXXXXXXXXXXXXX
        </div>
    </section>
    <section class="flex flex-wrap py4">
        <h3 class="col-12 center"> {{ .Home.Specs }}</h3>
        <div class="sm-col-6 flex flex-wrap">
            <div class="px2 col-12 sm-col-6 flex flex-column justify-center center">
                {{ template "piechart"}}
                <h6 class="shape-text">{{ .Home.Spec1 }}</h6>
            </div>
            <div class="px2 col-12 sm-col-6 flex flex-column justify-center center">
                {{ template "mindmap"}}
                <h6 class="shape-text">{{ .Home.Spec2 }}</h6>
            </div>
            <div class="px2 col-12 sm-col-6 flex flex-column justify-center center">
                {{ template "info"}}
                <h6 class="shape-text">{{ .Home.Spec3 }}</h6>
            </div>
            <div class="px2 col-12 sm-col-6 flex flex-column justify-center center">
                {{ template "lock"}}
                <h6 class="shape-text">{{ .Home.Spec4 }}</h6>
            </div>
        </div>
        <div class="col-6 flex items-center justify-center ">
             {{ template "globe"}}
        </div>
    </section>
    <section class="flex items-center justify-center bgblack txwhite">
        <h5 class="font-italic">" <span class="font-bold text-secondary"> {{ .Home.Moto1 }}</span>  {{ .Home.Moto2 }} "</h5>
    </section>
</main>
{{end}}